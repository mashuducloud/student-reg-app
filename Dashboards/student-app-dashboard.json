{
  "id": null,
  "title": "Student Registration App Metrics",
  "timezone": "browser",
  "schemaVersion": 37,
  "version": 1,
  "panels": [
    {
      "type": "stat",
      "title": "Total Student Registrations",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "student_registration_requests_total",
          "legendFormat": "Registrations"
        }
      ],
      "id": 1,
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 8,
        "h": 6
      }
    },
    {
      "type": "graph",
      "title": "Registrations Over Time",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(student_registration_requests_total[1m])",
          "legendFormat": "Requests/min"
        }
      ],
      "id": 2,
      "gridPos": {
        "x": 8,
        "y": 0,
        "w": 16,
        "h": 6
      }
    },
    {
      "type": "gauge",
      "title": "CPU Usage (seconds)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "process_cpu_seconds_total",
          "legendFormat": "CPU time"
        }
      ],
      "id": 3,
      "gridPos": {
        "x": 0,
        "y": 6,
        "w": 8,
        "h": 6
      }
    },
    {
      "type": "gauge",
      "title": "Memory Usage (MB)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "process_resident_memory_bytes / 1024 / 1024",
          "legendFormat": "Memory"
        }
      ],
      "id": 4,
      "gridPos": {
        "x": 8,
        "y": 6,
        "w": 8,
        "h": 6
      }
    },
    {
      "type": "graph",
      "title": "Garbage Collection Events",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(python_gc_collections_total[1m])",
          "legendFormat": "GC Generation {{generation}}"
        }
      ],
      "id": 5,
      "gridPos": {
        "x": 16,
        "y": 6,
        "w": 8,
        "h": 6
      }
    }
  ],
  "templating": {
    "list": []
  }
}